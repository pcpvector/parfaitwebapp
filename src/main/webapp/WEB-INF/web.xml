<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
	<listener-class>
             com.myvisualiq.ums.Listenerdemo 
        </listener-class>
   </listener>
  <servlet>
  	<servlet-name>SampleServlet</servlet-name>
